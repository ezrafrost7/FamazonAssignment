@model BookData

<tr>
    <td>@Model.Title</td>
    <td>@Model.AuthorLastName, @Model.AuthorFirstName @Model.AuthorMiddleInit</td>
    <td>@Model.Publisher</td>
    <td>@Model.ISBN</td>
    <td>@Model.Classification</td>
    <td>@Model.Category</td>
    <td>$@Model.Price</td>
    <td>@Model.PageNum pgs</td>
</tr>